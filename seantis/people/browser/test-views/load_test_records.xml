<model xmlns:security="http://namespaces.plone.org/supermodel/security" xmlns:people="http://namespaces.plone.org/supermodel/people" xmlns:marshal="http://namespaces.plone.org/supermodel/marshal" xmlns:form="http://namespaces.plone.org/supermodel/form" xmlns:indexer="http://namespaces.plone.org/supermodel/indexer" xmlns="http://namespaces.plone.org/supermodel/schema">
  <schema>
    <field name="lastname" type="zope.schema.TextLine" people:title="true" people:column="1" people:order="1">
      <title>Last Name</title>
    </field>
    <field name="firstname" type="zope.schema.TextLine" people:title="true" people:column="1" people:order="2">
      <title>First Name</title>
    </field>
    <field name="company" type="zope.schema.TextLine">
      <required>False</required>
      <title>Company</title>
    </field>
    <field name="address" type="zope.schema.TextLine">
      <required>False</required>
      <title>Address</title>
    </field>
    <field name="city" type="zope.schema.TextLine" people:column="2" people:selectable="true">
      <required>False</required>
      <title>City</title>
    </field>
    <field name="county" type="zope.schema.TextLine">
      <required>False</required>
      <title>County</title>
    </field>
    <field name="zip" type="zope.schema.TextLine">
      <required>False</required>
      <title>Zip</title>
    </field>
    <field name="state" type="zope.schema.TextLine" people:column="3" people:selectable="true">
      <required>False</required>
      <title>State</title>
    </field>
    <field name="phone" type="zope.schema.TextLine" people:column="4">
      <required>False</required>
      <title>Phone</title>
    </field>
    <field name="fax" type="zope.schema.TextLine">
      <required>False</required>
      <title>Fax</title>
    </field>
    <field name="email" type="seantis.plonetools.schemafields.Email" people:column="5">
      <required>False</required>
      <title>E-Mail</title>
    </field>
    <field name="web" type="seantis.plonetools.schemafields.Website">
      <required>False</required>
      <title>Web</title>
    </field>
  </schema>
</model>